<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="oval" >
            <gradient
                android:angle="90"
                android:startColor="#11de93"
                android:endColor="#25e1f6"
                android:type="linear" />
            <size android:width="24dp"
                android:height="24dp"/>
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval" >
            <solid android:color="@color/catskill_white" />
        </shape>
    </item>

</layer-list>